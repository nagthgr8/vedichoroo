<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mypubz.eportal" ios-CFBundleVersion="" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>126Astrology</name>
